{
    "manifest_version": 3,
    "name": "Daily Journal",
    "version": "1.0",
    "description": "Reminds you to provide a daily update at 6 PM.",
    "permissions": ["alarms", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
      "default_icon": {
        "128": "icon128.png"
      }
    },
    "icons": {
      "128": "icon128.png"
    },
    "options_page": "updates.html"
  }
  